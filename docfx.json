{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**.cs"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "globalMetadata": {
      "_appTitle": "USingleton",
      "_appFooter": "<span>Copyright Â© 2024 NK Studio<br>Generated by <strong>DocFX</strong> on 6 January 2024 </span>",
      "_appLogoPath": "images/logo.svg",
      "_enableSearch": true
    },
    "content": [
      {
        "files": [
          "**.yml",
          "**.md"
        ]
      },
      {
        "files": [
          "manual/**.yml",
          "manual/**.md"
        ]
      },
      {
        "files": [
          "api/**.yml",
          "api/**.md"
        ]
      },
      {
        "files": [
          "changelog/**.yml",
          "changelog/**.md"
        ]
      },
      {
        "files": [
          "license/**.yml",
          "license/**.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**",
          "code/**",
          "index.html"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "docs/**"
        ]
      }
    ],
    "dest": "docs",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "templates/NKStyle_Default"
    ],
    "postProcessors": [],
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}